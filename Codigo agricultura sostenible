public static void yuca(String pos1) {
    Scanner sc = new Scanner(System.in);
    int x = 0;
    System.out.println("1. Para saber el cuidado del cultivo en cuestion de hidratacion, 2. Para saber como se deben plantar, 3. Ambas.(Responda 1, 2 o 3 ) ");
    x = sc.nextInt();
    if (x == 1 ) {
        System.out.println("Se recomienda regar cada 10 dias ");
    }else if (x == 2) {
        System.out.println("Se deben tener en cuenta los siguientes pasos:");
        System.out.println( " - La yuca se planta mejor con esquejes de tallo. Corta esquejes de 20-30 centímetros de longitud de tallos maduros y saludables.");
        System.out.println( " - Planta los esquejes en surcos poco profundos y espaciados, enterrándolos verticalmente en el suelo.");
        System.out.println( " - Mantén el suelo húmedo pero no encharcado durante el proceso de enraizamiento.");

        
    }else if (x == 3) {
        System.out.println("Se deben tener en cuenta los siguientes pasos:");
        System.out.println( " - La yuca se planta mejor con esquejes de tallo. Corta esquejes de 20-30 centímetros de longitud de tallos maduros y saludables.");
        System.out.println( " - Planta los esquejes en surcos poco profundos y espaciados, enterrándolos verticalmente en el suelo.");
        System.out.println( " - Mantén el suelo húmedo pero no encharcado durante el proceso de enraizamiento.");
        System.out.println("- Finalmente, se recomienda regar cada 10 dias ");       
    }
    practicasSostenibles();
    sc.close();
}

public static void arroz(String pos1) {
    Scanner sc = new Scanner(System.in);
    int x = 0;
    System.out.println("1. Para saber el cuidado del cultivo en cuestion de hidratacion, 2. Para saber como se deben plantar, 3. Ambas.(Responda 1, 2 o 3 ) ");
    x = sc.nextInt();
    if (x == 1 ) {
        System.out.println("Se recomienda regar diario ");
    }else if (x == 2) {
        System.out.println("Se deben tener en cuenta los siguientes pasos:");
        System.out.println( " - Prepara el campo de arroz nivelando el terreno y asegurándote de que haya un sistema de riego adecuado.");
        System.out.println( " - Remoja las semillas de arroz en agua durante al menos 24 horas antes de plantarlas.");
        System.out.println( " - Planta las semillas de arroz en el campo inundado a una profundidad de alrededor de 2-3 centímetros.");
        System.out.println( " - Mantén el campo inundado con agua durante la temporada de crecimiento del arroz, asegurándote de que el agua no se estanque y que haya un buen drenaje.");


        
    }else if (x == 3) {
        System.out.println("Se deben tener en cuenta los siguientes pasos:");
        System.out.println( " - Prepara el campo de arroz nivelando el terreno y asegurándote de que haya un sistema de riego adecuado.");
        System.out.println( " - Remoja las semillas de arroz en agua durante al menos 24 horas antes de plantarlas.");
        System.out.println( " - Planta las semillas de arroz en el campo inundado a una profundidad de alrededor de 2-3 centímetros.");
        System.out.println( " - Mantén el campo inundado con agua durante la temporada de crecimiento del arroz, asegurándote de que el agua no se estanque y que haya un buen drenaje.");
        System.out.println("- Finalmente, se recomienda regar diario ");       
    }
    practicasSostenibles();
    sc.close();
}

public static void practicasSostenibles() {
    Scanner sc = new Scanner(System.in);
    String r;
    System.out.println("¿Desea saber recomendaciones para que el cultivo sea sostenible? ");
    r = sc.nextLine();
    if (r.equalsIgnoreCase("si")) {
        System.out.println("Algunas practicas sostenibles que puede implementar son: Utilizacion de metodos de cultivo organico, Conservacion de suelos, Manejo eficiente del agua y Proteccion de la biodiversidad ");

    } else {
        System.out.println("Si tienes alguna otra duda, repite el proceso ");

    }
    sc.close();
  }
}	
